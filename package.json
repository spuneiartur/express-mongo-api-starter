{
  "name": "express-mongo-api-starter",
  "version": "1.6.0",
  "description": "Starter for Node Express API projects with Mongo database",
  "main": "server.js",
  "scripts": {
    "build": "babel . --out-dir ./build",
    "start": "node ./build/server.js",
    "dev": "nodemon --exec babel-node --presets=@babel/preset-env -- ./server.js",
    "seed": "babel-node ./db/seed.js",
    "test": "mocha ./tests"
  },
  "keywords": [
    "express",
    "mongo",
    "mongodb",
    "api starter"
  ],
  "author": "victorocna <victor@chesscoders.com> (https://chesscoders.com)",
  "license": "MIT",
  "engines": {
    "npm": ">= 10",
    "node": ">= 22.8.0"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^2.23.0",
    "diacritic-regex": "^1.2.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "express-fileupload": "^1.4.0",
    "express-goodies": "^1.3.4",
    "helmet": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mongoose": "^5.13.20",
    "pino": "^6.13.1",
    "postmark": "^4.0.2",
    "validator": "^13.6.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.1",
    "@babel/node": "^7.23.9",
    "@babel/preset-env": "^7.24.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "chai": "^4.3.7",
    "eslint": "^8.57.0",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.1",
    "pino-pretty": "^4.8.0",
    "prettier": "^2.3.2"
  }
}
